[[inputs.httpjson]]
  name = "noc-mailsender"
  servers = [
  {{ with node - }}
    {{ range .Services - }}
        {{ if eq .Service "mailsender" - }}
            "http://{{ .Address }}:{{ .Port }}/mon/",
        {{ - end }}
     {{ - end }}
  {{ - end }}
  ]
  method = "GET"
  tag_keys = ["service", "instance"]
  tagexclude = ["server"]
