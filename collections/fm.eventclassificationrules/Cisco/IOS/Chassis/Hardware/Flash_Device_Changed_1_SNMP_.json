    "description": "CISCO-FLASH-MIB::ciscoFlashDeviceChangeTrap",
    "event_class__name": "Chassis | Hardware | Flash Device Changed",
    "preference": 1000,
    "patterns": [
        {
            "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
