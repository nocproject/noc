{
    "name": "IRE-Polus | Horizon | Chassis | Hardware | Module | Module Present (SNMP_TRAP)",
    "$collection": "fm.eventclassificationrules",
    "uuid": "14284223-f22f-4650-b99e-2a55b29cbccf",
    "event_class__name": "Chassis | Hardware | Module | Module Present",
    "preference": 1000,
    "sources": ["SNMP Trap"],
    "vars": [{
        "name": "module",
        "value": "=f\"{crate_id}/{slot_id})\""
    }],
    "patterns": [
        {
            "key_re": "^SNMPv2-MIB::snmpTrapOID.0$",
            "value_re": "^IPG-MIB::rack$"
        },
        {
            "key_re": "^IPG-MIB::cfgTrapCrateId$",
            "value_re": "^(?P<crate_id>.+)$"
        },
        {
            "key_re": "^IPG-MIB::cfgTrapSlot$",
            "value_re": "^(?P<slot_id>.+)$"
        },
        {
            "key_re": "^IPG-MIB::cfgTrapDevice$",
            "value_re": "^(?P<device>.*)$"
        },
        {
            "key_re": "^IPG-MIB::cfgTrapDescr$",
            "value_re": "^Alarm cleared:\\s+(?P<interface>.+)_SFP_State=Absent\\s*$"
        }
    ],
    "labels": [],
    "profiles": ["IRE-Polus.Horizon"]
}
