##------------------------------------------------
## noc-correlator default configuration.
##------------------------------------------------
## WARNING: Do not edit this file directly!!!
## Override settings using etc/noc-correlator.conf
##
[main]
logfile  = /var/log/noc/noc-correlator.log
loglevel = info
logsize  = 0
logfiles = 0
syslog_host =
pidfile  = /var/run/noc/noc-correlator.pid
#
max_job_faults = 0
# Limit concurrent SA tasks
# 0 - no limit
mrt_limit = 0


[debug]
enable_manhole = false
enable_timing = false
timing_base = local/timing

[correlator]
##
## Time window (in seconds) to search for active events
## in correlation loop
##
window = 10800
