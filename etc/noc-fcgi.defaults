##------------------------------------------------
## noc-fcgi default configuration.
##------------------------------------------------
## WARNING: Do not edit this file directly!!!
## Override settings using etc/noc-fcgi.conf
##
[main]
logfile  = /var/log/noc/noc-fcgi.log
loglevel = info
logsize  = 0
logfiles = 0
pidfile  = /var/log/noc/noc-fcgi.pid
heartbeat= off

[fcgi]
socket = /tmp/noc.fcgi
minspare = 2
maxspare = 5
maxrequests = 100
maxchildren = 20

