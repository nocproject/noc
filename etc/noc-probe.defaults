##------------------------------------------------
## noc-probe default configuration.
##------------------------------------------------
## WARNING: Do not edit this file directly!!!
## Override settings using etc/noc-probe.conf
##
[main]
logfile  = /var/log/noc/noc-probe.log
loglevel = info
logsize  = 0
logfiles = 0
pidfile  = /var/log/noc/noc-probe.pid
heartbeat= true

[path]
fping = /usr/bin/fping

[activator]
host = 127.0.0.1
port = 19704
local_address = 127.0.0.1
secret = secret