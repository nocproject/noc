    "description": "CISCO-SYSLOG-MIB::clogMessageGenerated",
    "event_class__name": "Network | Link | Link Down",
    "preference": 1000,
    "patterns": [
        {
            "key_re": "^source$",
            "value_re": "^SNMP Trap$"
        },
        {
            "key_re": "^profile$",
            "value_re": "^Cisco\\.IOS$"
        },
        {
            "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
