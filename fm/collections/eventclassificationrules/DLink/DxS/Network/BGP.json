[
    {
        "name": "DLink | DxS | Network | BGP | Established (SYSLOG)",
        "description": "INFO(6) [BGP(1):] BGP connection is successfully established Peer:<10.110.0.110>.",
        "event_class__name": "Network | BGP | Established",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?:INFO:|INFO\\(6\\)) \\[BGP(\\(1\\))?:\\] BGP connection is successfully established Peer:<(?P<peer>\\S+)>\\."
            }
        ]
    },

    {
        "name": "DLink | DxS | Network | BGP | Peer Down #1 (SYSLOG)",
        "description": "INFO(6) [BGP(2):] BGP connection is normally closed (Peer:<10.110.0.110>).",
        "event_class__name": "Network | BGP | Peer Down",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?:INFO:|INFO\\(6\\)) \\[BGP(\\(2\\))?:\\] BGP connection is normally closed \\(Peer:<(?P<peer>\\S+)>\\)\\."
            }
        ]
    },

    {
        "name": "DLink | DxS | Network | BGP | Peer Down #2 (SYSLOG)",
        "description": "WARN: [BGP:] BGP connection is closed due to Event: <10> (Peer:<10.0.0.1>).",
        "event_class__name": "Network | BGP | Peer Down",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?:WARN:|WARN\\(4\\)) \\[BGP(\\(2\\))?:\\] BGP connection is closed due to Event: <\\d+> \\(Peer:<(?P<peer>\\S+)>\\)\\."
            }
        ]
    },

    {
        "name": "DLink | DxS | Network | BGP | Peer Down #3 (SYSLOG)",
        "description": "WARN(4) [BGP(3):] BGP connection is closed due to error (Code:<2> Subcode:<7> Field:<Open Opt: Strict, Err-Data present> Peer:<192.168.1.1>).",
        "event_class__name": "Network | BGP | Peer Down",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?:WARN:|WARN\\(4\\)) \\[BGP(\\(3\\))?:\\] BGP connection is closed due to error \\(.+ Peer:<(?P<peer>\\S+)>\\)\\."
            }
        ]
    },

    {
        "name": "DLink | DxS | Network | BGP | Established (SNMP)",
        "description": "BGP4-MIB::bgpEstablished",
        "event_class__name": "Network | BGP | Established",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^BGP4-MIB::bgpEstablished$"
            },
            {
                "key_re": "^BGP4-MIB::bgpPeerState$",
                "value_re": "^BGP4-MIB::openconfirm$"
            },
            {
                "key_re": "^BGP4-MIB::bgpPeerRemoteAddr$",
                "value_re": "^(?P<ip>.+)$"
            }
        ]
    },

    {
        "name": "DLink | DxS | Network | BGP | Backward Transition (SNMP)",
        "description": "BGP4-MIB::bgpEstablished",
        "event_class__name": "Network | BGP | Backward Transition",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^BGP4-MIB::bgpBackwardTransition$"
            },
            {
                "key_re": "^BGP4-MIB::bgpPeerState$",
                "value_re": "^BGP4-MIB::established$"
            },
            {
                "key_re": "^BGP4-MIB::bgpPeerRemoteAddr$",
                "value_re": "^(?P<ip>.+)$"
            }
        ]
    }

]
