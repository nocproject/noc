[
    {
        "name": "DLink | DxS | Network | Link | Link Down (SYSLOG)",
        "description": "INFO: Port 17 link down",
        "event_class__name": "Network | Link | Link Down",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?:INFO:|INFO\\(6\\)) Port (?P<interface>.+) link down$"
            }
        ]
    },

    {
        "name": "DLink | DxS | Network | Link | Link Up (SYSLOG)",
        "description": "INFO: Port 17 link up, 100Mbps  FULL duplex",
        "event_class__name": "Network | Link | Link Up",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^syslog$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^message$",
                "value_re": "(?:INFO:|INFO\\(6\\)) Port (?P<interface>.+) link up, (?P<speed>\\S+)\\s+(?P<duplex>.+duplex)"
            }
        ]
    },

    {
        "name": "DLink | DxS | Network | Link | Link Down (SNMP)",
        "description": "SW36XXPRIMGMT-MIB::d.+",
        "event_class__name": "Network | Link | Link Down",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^(?:SW(?:3200|3x12SR|34XX|3500|3528|3800|3810|36XX|3700|6500|DGS3120|DGS3420|DGS3620)?PRIMGMT-(?:DES30XXP|DES3200)?-?MIB::[Dd].+|IF-MIB::linkDown)$"
            },
            {
                "key_re": "^IF-MIB::ifOperStatus\\.\\d+$",
                "value_re": "down$"
            },
            {
                "key_re": "^IF-MIB::ifIndex\\.\\d+$",
                "value_re": "(?P<interface>\\d+)$"
            }
        ]
    },

    {
        "name": "DLink | DxS | Network | Link | Link Up (SNMP)",
        "description": "SW36XXPRIMGMT-MIB::d.+",
        "event_class__name": "Network | Link | Link Up",
        "preference": 1000,
        "patterns": [
            {
                "key_re": "^source$",
                "value_re": "^SNMP Trap$"
            },
            {
                "key_re": "^profile$",
                "value_re": "^DLink\\.DxS$"
            },
            {
                "key_re": "^SNMPv2-MIB::snmpTrapOID\\.0$",
                "value_re": "^(?:SW(?:3200|3x12SR|34XX|3500|3528|3800|3810|36XX|3700|6500|DGS3120|DGS3420|DGS3620)?PRIMGMT-(?:DES30XXP|DES3200)?-?MIB::[Dd].+|IF-MIB::linkDown)$"
            },
            {
                "key_re": "^IF-MIB::ifOperStatus\\.\\d+$",
                "value_re": "up$"
            },
            {
                "key_re": "^IF-MIB::ifIndex\\.\\d+$",
                "value_re": "(?P<interface>\\d+)$"
            }
        ]
    },

    {
        "name": "DLink | DxS | Network | Link | DOM | Warning: Out of Threshold (SYSLOG)",
