{
  "name": "NOC.login",
  "version": "1.0.0.0",
  "indexHtmlPath": "index.html",
  "classpath": [
    "app"
  ],
  "overrides": [
    "overrides"
  ],
  "framework": "ext",
  "toolkit": "classic",
  "theme": "theme-triton",
  "requires": [
    "font-awesome"
  ],
  "fashion": {
    "inliner": {
      "enable": false
    }
  },
  "sass": {
    "namespace": "NOC.login",
    "etc": [
      "sass/etc/all.scss"
    ],
    "var": [
      "sass/var/all.scss",
      "sass/var"
    ],
    "src": [
      "sass/src"
    ]
  },
  "js": [
    {
      "path": "${framework.dir}/build/ext-all-rtl-debug.js"
    },
    {
      "path": "app.js",
      "bundle": true
    }
  ],
  "css": [
    {
      "path": "${build.out.css.path}",
      "bundle": true,
      "exclude": [
        "fashion"
      ]
    }
  ],
  "loader": {
    "cache": false,
    "cacheParam": "_dc"
  },
  "production": {
    "output": {
      "appCache": {
        "enable": true,
        "path": "cache.appcache"
      }
    },
    "loader": {
      "cache": "${build.timestamp}"
    },
    "cache": {
      "enable": true
    },
    "compressor": {
      "type": "yui"
    }
  },
  "testing": {
  },
  "development": {
    "tags": [
    ]
  },
  "bootstrap": {
    "base": "${app.dir}",
    "microloader": "bootstrap.js",
    "css": "bootstrap.css"
  },
  "output": {
    "base": "${workspace.build.dir}/${build.environment}/${app.name}",
    "appCache": {
      "enable": false
    }
  },
  "cache": {
    "enable": false,
    "deltas": true
  },
  "appCache": {
    "cache": [
      "index.html"
    ],
    "network": [
      "*"
    ],
    "fallback": []
  },
  "resources": [
    {
      "path": "tranclations",
      "output": "tranclations"
    }
  ],
  "ignore": [
    "(^|/)CVS(/?$|/.*?$)"
  ],
  "archivePath": "archive",
  "id": "1d6c7ddc-44b2-417f-9867-bf40758b42c9"
}
