{
    "name": "Report",
    "version": "0.0.0.1",
    "indexHtmlPath": "index.html",
    "classpath": [
        "app"
    ],
    "overrides": [
        "overrides"
    ],
    "framework": "ext",
    "requires": [
        "font-awesome",
        "ux",
        "charts"
    ],
    "fashion": {
        "inliner": {
            "enable": false
        }
    },
    "sass": {
        "namespace": "Report",
        "etc": [
            "sass/etc/all.scss"
        ],
        "var": [
            "sass/var/all.scss"
        ],
        "src": [
            "sass/src"
        ]
    },
    "js": [
        {
            "path": "app.js",
            "bundle": true
        }
    ],
    "classic": {
        "js": [
            {
                "path": "${framework.dir}/build/ext-all-rtl-debug.js"
            }
        ]
    },
    "css": [
        {
            "path": "${build.out.css.path}",
            "bundle": true,
            "exclude": [
                "fashion"
            ]
        }
    ],
    "loader": {
        "cache": false,
        "cacheParam": "_dc"
    },
    "production": {
        "output": {
            "base": "build",
            "appCache": {
                "enable": true,
                "path": "cache.appcache"
            }
        },
        "loader": {
            "cache": "${build.timestamp}"
        },
        "cache": {
            "enable": true
        },
        "compressor": {
            "type": "yui"
        }
    },
    "testing": {
        "output": {
            "base": "build",
            "appCache": {
                "enable": true,
                "path": "cache.appcache"
            }
        },
        "loader": {
            "cache": "${build.timestamp}"
        },
        "cache": {
            "enable": true
        }
    },
    "development": {
        "tags": []
    },
    "bootstrap": {
        "base": "${app.dir}",
        "manifest": "${build.id}.json",
        "microloader": "bootstrap.js",
        "css": "bootstrap.css"
    },
    "output": {
        "base": "${workspace.build.dir}/${build.environment}/${app.name}",
        "page": "index.html",
        "manifest": "${build.id}.json",
        "js": "${build.id}/app.js",
        "appCache": {
            "enable": false
        },
        "resources": {
            "path": "resources",
            "shared": "resources"
        }
    },
    "cache": {
        "enable": false,
        "deltas": "${build.id}/deltas"
    },
    "appCache": {
        "cache": [
            "index.html"
        ],
        "network": [
            "*"
        ],
        "fallback": []
    },
    "resources": [
        {
            "path": "resources",
            "output": "shared"
        }
    ],
    "ignore": [
        "(^|/)CVS(/?$|/.*?$)"
    ],
    "archivePath": "archive/${build.id}",
    "builds": {
        "classic": {
            "toolkit": "classic",
            "theme": "Report",
            "sass": {}
        }
    },
    "id": "a74a1d35-aabf-46df-9701-54f99dcd1e24"
}
