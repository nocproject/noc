{
  "name": "Report",
  "namespace": "Ext",
  "type": "theme",
  "extend": "theme-triton",
  "toolkit": "classic",
  "creator": "Oleg Pavlov",
  "summary": "Report theme",
  "detailedDescription": "Report theme",
  "version": "0.0.1",
  "compatVersion": "0.0.1",
  "format": "1",
  "slicer": {
    "js": [
      {
        "path": "${package.dir}/sass/example/custom.js",
        "isWidgetManifest": true
      }
    ]
  },
  "output": "${package.dir}/build",
  "local": true,
  "sass": {
    "namespace": "Ext",
    "etc": [
      "${package.dir}/sass/etc/all.scss"
    ],
    "var": [
      "${package.dir}/sass/var"
    ],
    "src": [
      "${package.dir}/sass/src"
    ]
  },
  "classpath": [
    "${package.dir}/src"
  ],
  "overrides": [
    "${package.dir}/overrides"
  ],
  "example": {
    "path": [
      "${package.dir}/examples"
    ]
  },
  "framework": "ext",
  "requires": []
}
